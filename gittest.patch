+        LocalDate ld = new LocalDate();
+        String day = ld.toString("dd");
+        String month = ld.toString("MM");
+        String year = ld.toString("yyyy");
+        
+        dayText.setText(day);
+        monthText.setText(month);
+        yearText.setText(year);
+    }
